    <Color x:Key="SecondaryForegroundColor">#555555</Color>
    <SolidColorBrush x:Key="SecondaryForegroundBrush" Color="{StaticResource SecondaryForegroundColor}" />

    <LinearGradientBrush x:Key="BackgroudGradientBrush" StartPoint="1 0">
        <GradientStop Offset="0" Color="{StaticResource BackgroudColor}" />
        <GradientStop Offset="1" Color="{StaticResource BackgroudColor2}" />
    </LinearGradientBrush>


    <LinearGradientBrush x:Key="GlassGradient" StartPoint="0,0" EndPoint="1,1.5">
        <GradientStop Offset="0" Color="{StaticResource 50_DefaultWhiteColor}" />
        <GradientStop Offset="0.35" Color="Transparent" />
    </LinearGradientBrush>


    <LinearGradientBrush x:Key="RedBlueGradientBrush">
        <GradientStop Offset="-0.5" Color="{StaticResource BlueColor}" />
        <GradientStop Offset="1.5" Color="{StaticResource DefaultRedColor}" />
        <GradientStop Offset="1.5" Color="{StaticResource BlueColor}" />
    </LinearGradientBrush>






</ResourceDictionary>